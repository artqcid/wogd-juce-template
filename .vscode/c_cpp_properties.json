{
    "configurations": [
        {
            "name": "JUCE ClangCL",
            "compilerPath": "clang-cl.exe",
            "cStandard": "c17",
            "cppStandard": "c++23",
            "intelliSenseMode": "windows-clang-x64",
            "compileCommands": "${workspaceFolder}/plugin/build/compile_commands.json",
            "includePath": [
                "${workspaceFolder}/plugin/source",
                "${workspaceFolder}/plugin",
                "${workspaceFolder}/../thirdParty/JUCE/modules",
                "${workspaceFolder}/../thirdParty/JUCE"
            ],
            "defines": [
                "JUCE_GLOBAL_MODULE_SETTINGS_INCLUDED=1"
            ]
        }
    ],
    "version": 4
}
